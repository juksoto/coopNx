@font-face {
  font-family: 'icomoon';
  src:  url('fonts/icomoon.eot?xtn25c');
  src:  url('fonts/icomoon.eot?xtn25c#iefix') format('embedded-opentype'),
    url('fonts/icomoon.ttf?xtn25c') format('truetype'),
    url('fonts/icomoon.woff?xtn25c') format('woff'),
    url('fonts/icomoon.svg?xtn25c#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="ico-"], [class*=" ico-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

